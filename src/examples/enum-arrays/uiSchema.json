{
    "type": "VerticalLayout",
    "elements": [
        { "type": "Control", "scope": "#/properties/enum_array", "options": { "variant": "checkbox" } },
        { "type": "Control", "scope": "#/properties/enum_array_horizontal", "options": { "variant": "checkbox", "horizontal": true } },
        { "type": "Control", "scope": "#/properties/enum_array_toggle", "options": { "variant": "toggleswitch", "horizontal": true } },
        { "type": "Control", "scope": "#/properties/enum_array_selectbutton", "options": { "variant": "selectbutton" } },
        { "type": "Control", "scope": "#/properties/enum_array_multiselect", "options": { "variant": "multiselect" } },
        { "type": "Control", "scope": "#/properties/enum_array_override", "options": { "variant": "multiselect", "enumValues": ["Belgium", "France", "Germany", "Netherlands"] } },
        { "type": "Control", "scope": "#/properties/enum_array_required" },
        { "type": "Control", "scope": "#/properties/enum_array_default" },
        { "type": "Control", "scope": "#/properties/enum_array_readonly", "options": { "readonly": true } }
